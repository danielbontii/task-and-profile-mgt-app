scalar LocalDateTime

type Query {
    allTodos: [Todo]
}

type User {
    id: ID!
    firstName: String!
    lastName: String!
    email: String!
    todos: [Todo]
    created_at: LocalDateTime!
    updated_at: LocalDateTime!
}

type Todo{
    title: String!
    description: String
    completed: Boolean!
    user: User!
    created_at: LocalDateTime!
    updated_at: LocalDateTime!
}