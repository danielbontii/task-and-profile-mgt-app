scalar LocalDateTime

type Query {
    allTodos: [Todo]
}

type User {
    id: ID!
    firstName: String!
    lastName: String!
    email: String!
    todos: [Todo]
    createdAt: LocalDateTime!
    updatedAt: LocalDateTime!
}

type Todo{
    id: ID!
    title: String!
    description: String
    completed: Boolean!
    user: User!
    createdAt: LocalDateTime!
    updatedAt: LocalDateTime!
}